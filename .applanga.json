{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "language": "en",
          "file_format": "nested_json",
          "path": "./app.json",
          "tag": "one"
        },
        {
          "language": "en",
          "file_format": "nested_json",
          "path": "./package-lock.json",
          "tag": "two"
        },
        {
          "language": "en",
          "file_format": "nested_json",
          "path": "./package.json",
          "tag": "three"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "exclude_languages": [
            "en"
          ],
          "file_format": "nested_json",
          "path": "./<language>.json",
          "tag": "one"
        },
        {
          "exclude_languages": [
            "en"
          ],
          "file_format": "nested_json",
          "path": "./<language>.json",
          "tag": "two"
        },
        {
          "exclude_languages": [
            "en"
          ],
          "file_format": "nested_json",
          "path": "./<language>.json",
          "tag": "three"
        }
      ]
    }
  }
}